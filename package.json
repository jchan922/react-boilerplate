{
	"name": "@jchan922/react-boilerplate",
	"version": "1.0.4",
	"description": "Boilerplate for Webpack, Express, & React",
	"main": "index.js",
	"scripts": {
		"dev": "nodemon ./src/server/server.dev.js",
		"build": "npm run webpack:prod && node ./src/server/server.prod.js",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:coverage": "jest --coverage --colors",
		"webpack:dev": "webpack --config ./build-utils/webpack.dev.js ",
		"webpack:prod": "webpack --config ./build-utils/webpack.prod.js"
	},
	"keywords": [],
	"author": "Justin Chan",
	"license": "ISC",
	"dependencies": {
		"express": "4.16.4",
		"react": "17.0.1",
		"react-dom": "17.0.1"
	},
	"devDependencies": {
		"@babel/core": "7.12.10",
		"@babel/preset-env": "7.12.11",
		"@babel/preset-react": "7.12.10",
		"@wojtekmaj/enzyme-adapter-react-17": "0.4.1",
		"babel": "6.23.0",
		"babel-eslint": "10.1.0",
		"babel-jest": "26.6.3",
		"babel-loader": "8.2.2",
		"clean-webpack-plugin": "3.0.0",
		"css-loader": "5.0.1",
		"enzyme": "3.11.0",
		"eslint": "7.18.0",
		"eslint-config-react": "1.1.7",
		"eslint-loader": "4.0.2",
		"eslint-plugin-react": "7.22.0",
		"html-webpack-plugin": "4.5.1",
		"jest": "26.6.3",
		"mini-css-extract-plugin": "1.3.4",
		"node-sass": "5.0.0",
		"nodemon": "2.0.7",
		"prettier": "2.2.1",
		"sass-loader": "10.1.1",
		"style-loader": "2.0.0",
		"webpack": "5.15.0",
		"webpack-cli": "4.3.1",
		"webpack-dev-server": "3.11.2",
		"webpack-hot-middleware": "2.25.0",
		"webpack-merge": "5.7.3"
	}
}
